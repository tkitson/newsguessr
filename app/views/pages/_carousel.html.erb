
 <div id="carouselExampleDark" class="carousel carousel-dark slide" data-interval="false">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <%= image_tag asset_path("1.jpg"), class: "front-page-img", data: { img_zoom_target: "img" } %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
    <div class="carousel-item">
      <%= image_tag asset_path("2.jpg"), class: "front-page-img img-hidden pages", data: { img_zoom_target: "img" } %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
    <div class="carousel-item">
      <%= image_tag asset_path("3.jpg"), class: "front-page-img img-hidden pages", data: { img_zoom_target: "img"} %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
    <div class="carousel-item">
      <%= image_tag asset_path("4.jpg"), class: "front-page-img img-hidden pages", data: { img_zoom_target: "img"} %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
    <div class="carousel-item">
      <%= image_tag asset_path("5.jpg"), class: "front-page-img img-hidden pages", data: { img_zoom_target: "img"} %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
    <div class="carousel-item">
      <%= image_tag asset_path("6.jpg"), class: "front-page-img img-hidden pages", data: { img_zoom_target: "img"} %>
      <div class="carousel-caption d-none d-md-block">
      </div>
    </div>
  </div>
  <div data-correct-or-not-target="buttons" class="img-hidden">
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
</div>
